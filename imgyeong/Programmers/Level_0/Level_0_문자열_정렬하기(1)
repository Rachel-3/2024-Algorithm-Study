import java.util.*;
class  Level_0_문자열_정렬하기(1) {
    public int[] solution(String my_string) {
        String nuStr = my_string.replaceAll("[^0-9]", "");
        int[] result = new int [nuStr.length()];
        for(int i = 0; i<nuStr.length(); i++){
            result[i]=nuStr.charAt(i)-'0';
        }
            Arrays.sort(result);
        return result;

     }
    }